IDD_FLATTABCTRL_DIALOG DIALOGEX 0, 0, 256, 140
STYLE DS_CENTER | WS_POPUP | WS_VISIBLE | WS_CLIPCHILDREN | WS_CAPTION | 
    WS_SYSMENU | WS_THICKFRAME
EXSTYLE WS_EX_APPWINDOW
CAPTION "Codejock Flat Tab Control Sample"
FONT 8, "MS Sans Serif"
BEGIN
    DEFPUSHBUTTON   "OK",IDOK,145,119,50,14
    PUSHBUTTON      "Cancel",IDCANCEL,199,119,50,14
    CONTROL         "",IDC_BORDER_TAB,"Static",SS_BLACKFRAME | NOT 
                    WS_VISIBLE,7,7,242,75,WS_EX_STATICEDGE
    CONTROL         "FTS_XT_HASARROWS",IDC_FTS_XT_HASARROWS,"Button",
                    BS_AUTOCHECKBOX | WS_GROUP | WS_TABSTOP,7,89,94,10
    CONTROL         "FTS_XT_HASHOMEEND",IDC_FTS_XT_HASHOMEEND,"Button",
                    BS_AUTOCHECKBOX | WS_TABSTOP,7,102,98,10
    CONTROL         "FTS_XT_HSCROLL",IDC_FTS_XT_HSCROLL,"Button",
                    BS_AUTOCHECKBOX | WS_TABSTOP,128,91,80,10
    CONTROL         "FTS_XT_BOTTOM",IDC_FTS_XT_BOTTOM,"Button",
                    BS_AUTOCHECKBOX | WS_TABSTOP,128,104,77,10
    PUSHBUTTON      "&Manipulate Tabs",IDC_MANIPULATE,7,119,81,14
END

IDD_FORMVIEW DIALOGEX 0, 0, 150, 86
STYLE WS_CHILD | WS_CLIPCHILDREN
FONT 8, "MS Sans Serif", 0, 0, 0x1
BEGIN
    GROUPBOX        "Favorite Season",IDC_GBOX_SEASON,7,7,136,71,0,
                    WS_EX_TRANSPARENT
    CONTROL         "Spring",IDC_RADIO1,"Button",BS_AUTORADIOBUTTON,17,22,
                    117,10
    CONTROL         "Summer",IDC_RADIO2,"Button",BS_AUTORADIOBUTTON,17,34,
                    117,10
    CONTROL         "Autumn",IDC_RADIO3,"Button",BS_AUTORADIOBUTTON,17,46,
                    117,10
    CONTROL         "Winter",IDC_RADIO4,"Button",BS_AUTORADIOBUTTON,17,58,
                    117,10
END

IDD_MANIPULATE DIALOG DISCARDABLE  0, 0, 260, 102
STYLE WS_POPUP | WS_CAPTION | WS_THICKFRAME
CAPTION "Manipulate The Tabs"
FONT 8, "MS Sans Serif"
BEGIN
    LTEXT           "Tab Number:",IDC_STATIC,7,10,42,8
